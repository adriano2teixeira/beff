{
  "name": "web-diff",
  "version": "0.1.3",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:web": "next build",
    "start": "next start",
    "lint": "next lint",
    "beff": "beff -p beff.json",
    "tsw": "tsc --noEmit --watch",
    "ts": "tsc --noEmit",
    "seed": "DATABASE_URL=postgresql://postgres_user:postgres_password@postgresql/postgres_db:5432 esno prisma/seed.ts",
    "migrate:dev": "DATABASE_URL=postgresql://postgres_user:postgres_password@postgresql/postgres_db:5432 npx prisma migrate dev",
    "wasm:dev": "wasm-pack build --dev --target bundler --out-dir pkg --out-name index",
    "wasm:release": "wasm-pack build --release --target bundler --out-dir pkg --out-name index"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.2",
    "@beff/cli": "workspace:^",
    "@beff/hono": "workspace:^",
    "@beff/react": "workspace:^",
    "@octokit/auth-app": "^6.0.0",
    "@prisma/client": "^5.3.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^1.1.2",
    "@tanstack/react-query": "^4.35.3",
    "@types/node": "^20.6.2",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.49.0",
    "eslint-config-next": "^13.5.1",
    "esno": "^0.17.0",
    "fuse.js": "^6.6.2",
    "highlight.js": "^11.8.0",
    "hono": "^3.6.3",
    "next": "^13.5.1",
    "next-auth": "^4.23.1",
    "next-themes": "^0.2.1",
    "octokit": "^3.1.0",
    "postcss": "^8.4.30",
    "radix-themes-tw": "^0.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "timeago.js": "4.0.0-beta.3",
    "typescript": "^5.2.2"
  },
