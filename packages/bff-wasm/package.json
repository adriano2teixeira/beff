  "main": "./dist-ext/extension.js",
  "scripts": {
    "build-rust": "wasm-pack build --dev --target nodejs",
    "build-ts": "esbuild ts-node/commandeer.ts --bundle --outfile=dist-cli/cli.js --platform=node --target=node14 --external:pnpapi --external:./pkg/hello_wasm.js --external:./pkg/hello_wasm_bg.js",
    "build-ext": "esbuild ts-node/extension.ts --bundle --outfile=dist-ext/extension.js --platform=node --target=node14 --external:pnpapi --external:vscode --external:./pkg/hello_wasm.js --external:./pkg/hello_wasm_bg.js",
    "build": "npm run build-rust && npm run build-ts && npm run build-ext",
    "wasm-test": "wasm-pack test --node",
    "test": "npm run build && node script/e2e.js && vitest",
    "do": "npm run build && node npm-bin index.js -p /home/lucas/fluff/bff/packages/bff-wasm/fixtures/codegen-snaps/hello-server/bff.json"
