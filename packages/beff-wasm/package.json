{
  "name": "beff-vscode",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist-ext/extension.js",
  "scripts": {
    "build-rust-release": "wasm-pack build --release --target nodejs",
    "build-rust": "wasm-pack build --dev --target nodejs",
    "build-rust-web": "wasm-pack build --dev --target bundler --out-dir ../../web/demo/src/pkg --out-name index",
    "build-cli": "esbuild ts-node/commandeer.ts --bundle --outfile=dist-cli/cli.js --platform=node --target=node14 --external:pnpapi --external:./pkg/beff_wasm.js --external:./pkg/beff_wasm_bg.js",
    "build-ext": "esbuild ts-node/extension.ts --bundle --outfile=dist-ext/extension.js --platform=node --target=node14 --external:pnpapi --external:vscode --external:./pkg/beff_wasm.js --external:./pkg/beff_wasm_bg.js",
    "build": "npm run build-rust && npm run build-rust-web && npm run build-cli && npm run build-ext",
    "test": "node script/e2e.js && vitest --run --update",
    "do": "npm run build && node npm-bin/index.js -v -p /home/lucas/fluff/bff/packages/beff-wasm/vitest/export-decoder/bff.json",
    "stress": "npm run build-rust-release && time node npm-bin/index.js -v -p /home/lucas/fluff/bff/packages/example-blog/bff.json",
    "ts": "tsc --noEmit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@beff/cli": "workspace:^1.0.0",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@babel/code-frame": "^7.22.13",
    "@types/babel__code-frame": "^7.0.3",
    "@types/node": "^20.5.7",
    "@types/vscode": "^1.81.0",
    "@beff/hono": "workspace:^1.0.0",
    "commander": "^11.0.0",
    "esbuild": "^0.19.2",
    "hono": "^3.5.6",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  }
}