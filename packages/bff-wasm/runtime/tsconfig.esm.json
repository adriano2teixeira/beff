{
  "compilerOptions": {
    "target": "ES2021",
    "lib": ["ES2021"],
    "module": "ESNext",
    "moduleResolution": "node",
    "sourceMap": false,
    "composite": false,
    "declaration": true,
    "strict": true,
    "alwaysStrict": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "baseUrl": "./",
    "outDir": "./dist/esm",
    "noFallthroughCasesInSwitch": true
  },
  "include": ["./runtime.ts"]
}
